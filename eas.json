{
  "cli": {
    "version": ">= 0.55.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {},
      "env": {
        "APP_ENV": "development",
        "APP_NAME": "CatHot Dev",
        "IOS_BUNDL_ID": "com.darkosoft.cathot.dev",
        "ANDROID_PKG": "com.darkosoft.cathot.dev",
        "CATHOT_API_URL": "https://cathot-development.up.railway.app/api"
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "APP_ENV": "preview",
        "APP_NAME": "CatHot Prev",
        "IOS_BUNDL_ID": "com.darkosoft.cathot.prev",
        "ANDROID_PKG": "com.darkosoft.cathot.prev",
        "CATHOT_API_URL": "https://cathot-development.up.railway.app/api"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "APP_NAME": "CatHot",
        "IOS_BUNDL_ID": "com.darkosoft.cathot",
        "ANDROID_PKG": "com.darkosoft.cathot",
        "CATHOT_API_URL": "https://cathot.up.railway.app/api"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
